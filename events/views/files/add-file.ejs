<% include ../partials/header %>

<div class="row">
    <form action='<%= baseURL %>fileupload' method="post" class="col s12" encType="multipart/form-data">
        <input type="hidden" name="file_upload_id" value="">
        <div class="form-group row">
            <label for="sampleFile" class="col-2 col-form-label">File </label>
            <div class="col-10">
                <input type="file" name="sampleFile" value="" class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <input type="submit" name="submit" value="Save" class="btn btn-primary btn-lg" />
        </div>
    </form>
</div>


<% include ../partials/footer %>